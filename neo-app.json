{
	"welcomeFile": "/webapp/index.html",
	"routes": [{
		"path": "/resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/resources"
		},
		"description": "SAPUI5 Resources"
	}, {
		"path": "/test-resources",
		"target": {
			"type": "service",
			"name": "sapui5",
			"entryPath": "/test-resources"
		},
		"description": "SAPUI5 Test Resources"
	}, {
		"path": "/sfsfdataservice",
		"target": {
			"type": "destination",
			"name": "sfsfdataservice"
		},
		"description": "dataservice"
	}, {
      "path": "/services/userapi",
      "target": {
        "type": "service",
        "name": "userapi"
      }
    }, {
		"path": "/pmapi",
		"target": {
			"type": "destination",
			"name": "pmapi"
		},
		"description": "dataservice"
	}],
	"sendWelcomeFileRedirect": true,
	"cacheControl": [{
		"directive": "private",
		"maxAge": 0,
		"path": "*html"
	}, {
		"directive": "private",
		"maxAge": 0,
		"path": "sap-ui-cachebuster-info.json"
	}, {
		"directive": "public",
		"maxAge": 31536000
	}]
}