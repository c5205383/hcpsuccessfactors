<mvc:View controllerName="hcpsuccessfactors.controller.MainApp" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<SplitApp id="sfsf-ext-app">
		<masterPages>
			<Page id="main-shell-master" title="{i18n>main-shell-master-title}" icon="sap-icon://action" class="sapUiStdPage" showNavButton="false">
				<content>
					<List items="{path:'/category'}">
						<items>
							<StandardListItem title="{name}" type="Active" press="onItemPress" />
						</items>
					</List>
				</content>
			</Page>
		</masterPages>
	</SplitApp>
</mvc:View>