<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<ResponsivePopover title= "Choose to display" class="sapUiPopupWithPadding" placement="Bottom">
		<beginButton>
			<Button text="Save" press="handleSaveButton"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="handleCloseButton"/>
		</endButton>
		<content>
			<l:VerticalLayout class="sapUiSmallMargin">
				<CheckBox text="{i18n>myform-table-formtitle}" selected="true" id="xz"/>
				<CheckBox text="{i18n>myform-table-employee}" selected="true"/>
				<CheckBox text="{i18n>myform-table-stepduedate}" selected="true"/>
				<CheckBox text="{i18n>myform-table-formstartdate}" selected="true"/>
				<CheckBox text="{i18n>myform-table-formenddate}" selected="true"/>
				<CheckBox text="{i18n>myform-table-formduedate}" selected="true"/>
				<CheckBox text="{i18n>myform-table-lastmodified}" selected="true"/>
			</l:VerticalLayout>
		</content>
	</ResponsivePopover>
</core:FragmentDefinition>