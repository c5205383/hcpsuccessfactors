<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:uxap="sap.uxap" height="100%"
	controllerName="hcpsuccessfactors.controller.detail.myForm" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>myform-title}">
				<Toolbar height="3rem">
					<Title text="{i18n>MyFormTitle}"/>
				</Toolbar>
				<content>
					<Button text="Toggle Collapse/Expand" icon="sap-icon://menu2" press="onCollapseExapandPress"/>
					<tnt:NavigationList id="navigationList" width="250px">
						<tnt:NavigationListItem text="All Forms" icon="sap-icon://form"></tnt:NavigationListItem>
						<tnt:NavigationListItem text="In Process" icon="sap-icon://pending">
							<tnt:NavigationListItem text="Inbos"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="En Route"></tnt:NavigationListItem>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Completed" icon="sap-icon://complete">
							<tnt:NavigationListItem text="Create New Folder"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="360 Reviews"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Goal Plans"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Job Description Reviews"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Performance Reviews"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Stack Rankings"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Succession Nominations"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Talent Reviews"></tnt:NavigationListItem>
						</tnt:NavigationListItem>
						<tnt:NavigationListItem text="Form Status" icon="sap-icon://order-status">
							<tnt:NavigationListItem text="Aggregrate"></tnt:NavigationListItem>
							<tnt:NavigationListItem text="Individual"></tnt:NavigationListItem>
						</tnt:NavigationListItem>
					</tnt:NavigationList>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>