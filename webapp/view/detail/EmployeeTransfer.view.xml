<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="hcpsuccessfactors.controller.detail.EmployeeTransfer" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="{i18n>empTransfer-title}">
				<content>
				    <HBox justifyContent="SpaceAround" alignItems="Center">
				        <Text text="{i18n>empTransfer-choosePartnerId}:" />
				        <Select >
				            <items>
				            </items> 
				        </Select>
				        <Text text="{i18n>empTransfer-chooseTransCity}:" />
				        <Select >
				            <items>
				            </items> 
				        </Select>
				        <Text text="{i18n>empTransfer-chooseTransBranch}:" />
				        <Select >
				            <items>
				            </items> 
				        </Select>
				        <Button text="{i18n>empTransfer-add}" />
				    </HBox>
				    <Table >
				        <columns>
							<Column ><Text text="{i18n>empTransTable-id}"/></Column>
							<Column ><Text text="{i18n>empTransTable-zhname}"/></Column>
							<Column ><Text text="{i18n>empTransTable-position}"/></Column>
							<Column ><Text text="{i18n>empTransTable-city}"/></Column>
							<Column ><Text text="{i18n>empTransTable-preSubbranch}"/></Column>
							<Column ><Text text="{i18n>empTransTable-transCity}"/></Column>
							<Column ><Text text="{i18n>empTransTable-transSubbranch}"/></Column>
							<Column ><Text text="{i18n>empTransTable-effectiveDate}"/></Column>
						</columns>
						<items>
							<ColumnListItem type="Active" press="onItemPress">
								<cells>
								</cells>
							</ColumnListItem>
						</items>
				    </Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>