								<layoutData><l:SplitterLayoutData size="325px" resizable="false"/></layoutData>
							</Table>
							<Table inset="false" backgroundDesign="Solid">
								<headerToolbar>
									<Toolbar>
										<Title text="LPM" level="H2"/>
										<ToolbarSpacer/>
										<Button icon="sap-icon://add"/>
									</Toolbar>
								</headerToolbar>
								<columns>
									<Column></Column>
								</columns>
								<items>
