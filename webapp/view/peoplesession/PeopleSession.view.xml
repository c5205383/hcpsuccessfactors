											<Label text="Competency" textAlign="Center" width="100%"/>
										</t:multiLabels>
										<t:template>
											<Text text="{category}"/>
										</t:template>
									</t:Column>
									<t:Column width="84px" resizable="false">
										<t:multiLabels>
											<Label text="Competency"/>
