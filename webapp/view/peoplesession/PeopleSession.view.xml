								<layoutData><l:SplitterLayoutData size="190px" resizable="false"/></layoutData>
							</Table>
						</l:contentAreas>
					</l:Splitter>
					<l:Splitter orientation="Vertical">
						<l:layoutData><l:SplitterLayoutData size="505px" resizable="false"/></l:layoutData>
						<l:contentAreas>
							<t:Table id="treeTable" enableColumnReordering="false" allowColumnReordering="false" selectionMode="None" rows="{/catalog}"
