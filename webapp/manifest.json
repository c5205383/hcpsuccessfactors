{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "hcpsuccessfactors",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "hcpsuccessfactors.view.MainApp",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "hcpsuccessfactors.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "hcpsuccessfactors.view",
				"controlId": "sfsf-ext-app",
				"controlAggregation": "detailPages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "myGoal"
				},
				{
					"pattern": "detail1",
					"name": "detail001",
					"target": "detail001"
				},
				{
					"pattern": "detail2",
					"name": "detail002",
					"target": "detail002"
				},
				{
					"pattern": "mygoal",
					"name": "myGoal",
					"target": "myGoal"
				},
				{
					"pattern": "myform",
					"name": "myForm",
					"target": "myForm"
				},
				{
					"pattern": "mygoal/goals/{id}",
					"name": "goalDetail",
					"target": "goalDetail"
				},
				{
					"pattern": "myprofile",
					"name": "myProfile",
					"target": "myProfile"
				},
				{
					"pattern": "employeecenter",
					"name": "employeeCenter",
					"target": "employeeCenter"
				},
				{
					"pattern": "employeecenter/profile/{id}",
					"name": "userDetail",
					"target": "profileDetail"
				},
				{
					"pattern": "candidate",
					"name": "candidate",
					"target": "candidate"
				},
				{
					"pattern": "myform/formDetail/{id}",
					"name": "formdetail",
					"target": "formDetail"
				},
				{
					"pattern": "emptransfer",
					"name": "empTransfer",
					"target": "empTransfer"
				},
				{
					"pattern": "hiringprocess",
					"name": "hiringProcess",
					"target": "hiringProcess"
				}
			],
			"targets": {
				"App": {
					"viewName": "MainApp"
				},
				"detail001": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "DemoDetail001"
				},
				"detail002": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "DemoDetail002"
				},
				"myGoal": {
					"viewPath": "hcpsuccessfactors.view.detail.goal",
					"viewName": "MyGoal"
				},
				"goalDetail": {
					"viewPath": "hcpsuccessfactors.view.detail.goal",
					"viewName": "GoalDetail"
				},
				"myForm": {
					"viewPath": "hcpsuccessfactors.view.detail.form",
					"viewName": "MyForm"
				},
				"myProfile": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "MyProfile"
				},
				"employeeCenter": {
					"viewPath": "hcpsuccessfactors.view.detail.empCenter",
					"viewName": "EmployeeCenter"
				},
				"profileDetail": {
					"viewPath": "hcpsuccessfactors.view.detail.empCenter",
					"viewName": "ProfileDetail"
				},
				"candidate": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "Candidate"
				},
				"empTransfer": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "EmployeeTransfer"
				},
				"formDetail": {
					"viewPath": "hcpsuccessfactors.view.detail.form",
					"viewName": "Formdetail"
				},
				"hiringProcess": {
					"viewPath": "hcpsuccessfactors.view.detail",
					"viewName": "HiringProcess"
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}